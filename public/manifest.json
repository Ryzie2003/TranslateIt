{
  "manifest_version": 3,
  "name": "StreamTranslations",
  "version": "1.0.0",
  "description": "Translate across the web",
  "icons": {
    "32": "icon32.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://www.netflix.com/*"
  ]
}